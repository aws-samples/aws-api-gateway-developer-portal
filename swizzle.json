{
	"files": [
		"package.json",
		"dev-portal/package.json",
		"dev-portal/public/apigateway-js-sdk/apigClient.js"
	],
	"params": [
		{
			"name": "artifactsS3Bucket",
			"description": "existing or new bucket name for storing deployment artifacts",
			"defaultValue": "YOUR_ARTIFACTS_BUCKET_NAME",
			"regex": {
				"only alpha numerical values between 1 and 255 characters with dots, dashes and underscores": "^[a-zA-Z0-9._-]{1,255}$"
			}
		},
		{
			"name": "siteS3Bucket",
			"description": "aws S3 bucket name to use for application website",
			"defaultValue": "YOUR_CLIENT_BUCKET_NAME",
			"regex": {
				"only alpha numerical values between 1 and 255 characters with dots, dashes or underscores": "^[a-zA-Z0-9._-]{1,255}$"
			}
		},
		{
			"name": "accountId",
			"description": "your aws account id",
			"defaultValue": "YOUR_ACCOUNT_ID",
			"regex": {
				"must be 12 characters long": "^.{12}$"
			}
		},
		{
			"name": "primaryAwsRegion",
			"description": "aws region where to deploy this application stack",
			"defaultValue": "YOUR_PRIMARY_AWS_REGION",
			"choices": [
				"us-east-1",
				"us-west-2",
				"eu-west-1",
				"eu-central-1",
				"ap-northeast-1",
				"ap-northeast-2"
			]
		},
		{
			"name": "cloudFormationStackName",
			"description": "stack name used as prefix for all resources generated by cloud formation",
			"defaultValue": "YOUR_CLOUDFORMATION_STACK_NAME",
			"regex": {
				"must start with a letter": "^[a-zA-Z]{1}",
				"only alpha numerical values with dashes": "^[a-zA-Z0-9-]*$"
			}
		},
		{
			"name": "apiGatewayApiName",
			"description": "aws api gateway display name",
			"defaultValue": "YOUR_API_GATEWAY_API_NAME",
			"generated": true
		},
		{
			"name": "apiGatewayApiId",
			"description": "aws api gateway id",
			"defaultValue": "YOUR_API_GATEWAY_API_ID",
			"generated": true
		},
		{
			"name": "cognitoRegion",
			"description": "aws region where cognito pool is hosted",
			"defaultValue": "YOUR_COGNITO_REGION",
			"generated": true
		},
		{
			"name": "cognitoUserPoolId",
			"description": "cognito user pool id",
			"defaultValue": "YOUR_COGNITO_USER_POOL_ID",
			"generated": true
		},
		{
			"name": "cognitoClientId",
			"description": "cognito client id",
			"defaultValue": "YOUR_COGNITO_CLIENT_ID",
			"generated": true
		},
		{
			"name": "cognitoIdentityPoolId",
			"description": "cognito identity pool id",
			"defaultValue": "YOUR_COGNITO_IDENTITY_POOL_ID",
			"generated": true
		},
		{
			"name": "homepage",
			"description": "application web site homepage url",
			"defaultValue": "YOUR_APP_HOMEPAGE",
			"generated": true
		}
	]
}
